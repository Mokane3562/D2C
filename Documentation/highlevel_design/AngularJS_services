--courses--
--object returned and data supplied are in the form of--
--a course JSON object as described in the javascriptObjects.txt file--
getCourse(course_id): object
postCourse(course_id, data): void

--files--
--object returned and data supplied are in the form of--
--a file JSON object as described in the javascriptObjects.txt file--
getFile(course_id, assignment, file_name): object
postFile(course_id, assignment, file_name, data): void

--assignments--
--object returned and data supplied are in the form of--
--an assignment JSON object as described in the javascriptObjects.txt file--
getAssignment(course_id, assignment): object
postAssignment(course_id, assignment, data): void

--grades--
--object returned and data supplied are in the form of--
--a grade JSON object as described in the javascriptObjects.txt file--
getGrade(course_id, student_user_name, assignment): object
postGrade(course_id, student_user_name, assignment, grade): void
releaseGrades(course_id, assignment): void

--student--
--object returned and data supplied are in the form of--
--a student JSON object as described in the javascriptObjects.txt file--
getStudent(student_user_name): object
postStudent(student_user_name, data): void

--TA--
--object returned and data supplied are in the form of--
--a TA JSON object as described in the javascriptObjects.txt file--
getTA(student_user_name, course_id): object
postTA(student_user_name, course_id, data): void

--Professor--
--object returned and data supplied are in the form of--
--a prof JSON object as described in the javascriptObjects.txt file--
getProfessor(prof_user_name): object
postProfessor(prof_user_name, data): void

--Submission-- 
--object returned and data supplied are in the form of--
--a submission JSON object as described in the javascriptObjects.txt file--
getSubmission(course_id, student_user_name, assignment): object
postSubmission(course_id, student_user_name, assignment, data): void
